{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "allowCompoundWords": true,
  "dictionaries": [
    "software-terms",
    "npm",
    "fullstack",
    "redis",
    "typescript",
    "softwareTerms",
    "node",
    "en_us",
    "en-gb",
    "html",
    "companies",
    "misc",
    "filetypes",
    "bash",
    "lorem-ipsum",
    "es-es"
  ],
  "files": ["**", ".vscode/**", ".github/**"],
  "ignorePaths": [
    "pnpm-lock.yaml",
    ".pnpm-store/",
    "packages/db/src/migrations/**",
    "packages/i18n/src/messages/*.json",
    "packages/db/migrations/**",
    "packages/eslint-config/eslint.config.bundled_*.mjs",
    "REST.http",
    "README.md",
    "docs/**"
  ],
  "ignoreRegExpList": ["apiKey='[a-zA-Z0-9-]{32}'"],
  "import": [
    "@cspell/dict-typescript",
    "@cspell/dict-software-terms",
    "@cspell/dict-node",
    "@cspell/dict-en_us",
    "@cspell/dict-en-gb",
    "@cspell/dict-npm",
    "@cspell/dict-html",
    "@cspell/dict-companies",
    "@cspell/dict-filetypes",
    "@cspell/dict-bash",
    "@cspell/dict-lorem-ipsum/cspell-ext.json",
    "@cspell/dict-es-es/cspell-ext.json"
  ],
  "language": "en",
  "useGitignore": true,
  "version": "0.2",
  "words": [
    "Acesso",
    "bitnami",
    "Bullmq",
    "Dimensões",
    "greenacesso",
    "Importação",
    "isready",
    "linha",
    "planilha",
    "Português",
    "rharkor",
    "sonarjs",
    "Timestamptz",
    "willgravinadev"
  ]
}
